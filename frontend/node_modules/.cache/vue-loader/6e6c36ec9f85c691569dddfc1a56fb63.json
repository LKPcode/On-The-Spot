n            \n              <div class=\"info\">\n                <p> {{$number( asset.asset_amount) }}  {{asset.asset}} </p>\n                <p> {{$number(asset.asset_amount_to_usd)}} USD </p>\n                <p> {{$number(asset.asset_percentage)}} % </p>\n              </div>\n            </div>\n          </div>\n          <div class=\"buy\">\n           <div class=\"asset\" v-for=\" asset in $store.state.buy_assets\" :key=\"asset.asset\">\n              <div class=\"asset-name\">\n\n                  <img  :src=\"$server + '/static/'+ asset.asset.toLowerCase()  +  '.svg'\" alt=\"\">\n              <div class=\"asset-name\"> {{asset.asset}} </div>\n              </div>\n            \n              <div class=\"info\">\n                <p> {{$number( asset.asset_amount) }}  {{asset.asset}} </p>\n                <p> {{$number(asset.asset_amount_to_usd)}} USD </p>\n                <p> {{$number(asset.asset_percentage)}} % </p>\n              </div>\n            </div>\n          </div>\n        </div>\n\n         <button @click=\"confirmSwap()\" class=\"swap-button confirm-button\" role=\"button\">Confirm Swap (${{$number($store.state.sell_amount)}})\n\n          \n         </button>\n      </div>\n\n  </div>\n</template>\n\n<script>\nexport default {\n  methods:{\n    confirmSwap(){\n      this.$axios\n        .post(this.$server + \"/api/swap\", {sell: this.$store.state.sell_assets,\n                                            buy: this.$store.state.buy_assets} )\n        .then( response => {\n            console.log(response)\n            //this.$store.commit(\"setWalletData\", response.data)\n            }\n        );\n    }\n  }\n\n}\n</script>\n\n<style>\n\n\n.blur{\n    position: fixed;\n    top:0;\n    width:100%;\n    height:100%;\n    transition: 1s backdrop-filter linear;\n    /* filter: blur(60px); */\n   backdrop-filter: blur(30px);\n    z-index: 100;\n}\n.content{\n  width: 500px;\n  height:600px;\n  background-color: black;\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  transform: translate(-50%,-50%);\n  padding: 1rem;\n\n  display: flex;\n  flex-direction: column;\n\n  text-align: center;\n}\n\n.confirm-button{\n  display: block;\n  margin: 1rem;\n  margin-top: auto;\n  \n}\n\n.trade-data{\n  display: flex;\n  justify-content: space-around;\n  flex-wrap:wrap\n}\n.title{\n  font-weight: bold;\n  margin-top:0px;\n  position: sticky;\n  top: 0;\n\n}\nimg{\n  width: 60px;\n}\n\n.asset{\n  display: flex;\n  gap:1;\n  justify-content: space-around;\n  border-bottom: 2px solid gray;\n}\n.asset-name{\n  font-weight: bold;\n  align-self: center;\n  margin:1rem;\n\n}\n.info{\n  margin: 1rem 0rem;\n  \n}\n\n.buy{\n  border-left: 2px solid gray;\n  flex: 1;\n  padding: 1rem;\n   height: 430px;\n  flex: 1;\n  padding: 1rem;\n  padding-left: 0px;\n  overflow: scroll ;\n}\n.sell{\n  height: 430px;\n  flex: 1;\n  padding: 1rem;\n  padding-right: 0px;\n  overflow: scroll ;\n  \n\n}\n/* Hide scrollbar for Chrome, Safari and Opera */\n.sell::-webkit-scrollbar {\n  display: none;\n}\n/* Hide scrollbar for Chrome, Safari and Opera */\n.buy::-webkit-scrollbar {\n  display: none;\n}\n\n/* Hide scrollbar for IE, Edge and Firefox */\n.sell, .buy {\n  -ms-overflow-style: none;  /* IE and Edge */\n  scrollbar-width: none;  /* Firefox */\n}\n\n.headings{\n  width: 100%;\n  display: flex;\n  margin-bottom: 1rem;\n  /* justify-content: center; */\n}\n.headings h3{\n  display:inline;\n  margin:auto;\n\n}\n\n\n\n</style>"]}]}