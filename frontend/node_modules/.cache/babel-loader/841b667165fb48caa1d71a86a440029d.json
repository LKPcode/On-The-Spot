t'\"\n               :height=\"'500px'\"\n               :width=\"5\"\n              \n    >\n    <template v-slot:process=\"{ style}\">\n          <div class=\"vue-slider-process\" :style=\"style\">\n            <div :class=\"[\n              'merge-tooltip',\n            ]\">\n              <div v-for=\"coin in coins\" :key=\"coin\">\n                  <img class=\"logo scale-up-center\" :src='\"../../public/\" +  coin + \".svg\"' style=\"width:30px;\" >\n\n              </div>\n            </div>\n          </div>\n\n        \n\n        </template>\n    <template #tooltip=\"{ value }\">\n        <div class=\"tooltip\">\n            {{value}}%\n          <!-- <img src=\"../../public/aave.svg\" style=\"width:30px;\" > -->\n          </div>\n       \n      </template>\n    \n    </vue-slider>\n  </div>\n</template>\n\n<script>\nimport VueSlider from 'vue-slider-component'\nimport 'vue-slider-component/theme/default.css'\n\n\n\nexport default {\n  name: 'GeneralSlider',\n  components: {\n    VueSlider\n  },\n   data: function () {\n      return {\n        value: [25],\n        formatter: '{value}%',\n         process: dotsPos => [\n          [ dotsPos[0],0, { backgroundColor: 'black' }],\n         \n          \n        ],\n        coins: [\"aave\",\"dot\"]\n      }\n   }\n \n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.merge-tooltip {\n      position: absolute;\n      right: 30px;\n      top: 50%;\n      transform: translate(75px, -50%);\n    }\n\n.merge-tooltip-percentage {\n      position: absolute;\n      left: -25px;\n      top: 50%;\n      transform: translate(-105px, 50%);\n}\n.tooltip{\n  position: absolute;\n  left: -35px;\n  top: -15px;\n  padding: 0.2rem;\n  border-radius: 1rem;\n  background: rgba(255, 255, 255, 0.8);\n\t-webkit-backdrop-filter: blur(5px);\n\tbackdrop-filter: blur(5px);\n  border: 1px solid rgba(255,255,255,0.2);\n}\n\n  \n</style>\n"],"sourceRoot":"src/components"}]}